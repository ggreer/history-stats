{
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_popup": "popup.html",
        "default_title": "History Stats"
    },
    "description": "Show pretty graphs.",
    "homepage_url": "https://github.com/ggreer/history-stats",
    "icons": {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png"
    },
    "manifest_version": 2,
    "name": "History Stats",
    "permissions": [
        "<all_urls>",
        "chrome://favicon/",
        "history",
        "tabs"
    ],
    "version": "0.1"
}
